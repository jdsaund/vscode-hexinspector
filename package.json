{
	"name": "ethconverter",
	"displayName": "EthConverter",
	"description": "Provides fast and easy way to peek other forms of an ether value",
	"version": "1.5.1",
	"publisher": "mateuszchudyk",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/mateuszchudyk/vscode-hexinspector"
	},
	"keywords": [
		"hex",
		"inspector",
		"hover",
		"converter"
	],
	"engines": {
		"vscode": "^1.30.0"
	},
	"icon": "images/icon.png",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "EthConverter",
			"properties": {
				"hexinspector.inputDataTypes": {
					"type": "array",
					"default": [
						"hexadecimal",
						"binary",
						"decimal"
					]
				},
				"hexinspector.hoverContent": {
					"type": "array",
					"default": [
						"decimal",
						"hexadecimal",
						"binary",
						"float32",
						"ascii",
						"size"
					]
				},
				"hexinspector.endianness": {
					"type": "string",
					"default": "little",
					"description": "Little Endian or Big Endian?"
				}
			}
		},
		"languages": [
			{
				"id": "hexinspector"
			}
		],
		"grammars": [
			{
				"language": "hexinspector",
				"scopeName": "source.hexinspector",
				"path": "./syntaxes/hexinspector.json"
			}
		]
	},
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^18.19.54",
		"tslint": "^5.8.0",
		"typescript": "^5.6.2",
		"vscode": "^1.1.25"
	},
	"dependencies": {
		"bignumber.js": "^9.1.2",
		"undici": "^6.19.8"
	}
}
