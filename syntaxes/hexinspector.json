{
	"scopeName": "source.hexinspector",
	"fileTypes": [
		"hexinspector"
	],
	"patterns": [
		{
			"include": "#title"
		},
		{
			"include": "#number"
		},
		{
			"include": "#subheading"
		},
		{
			"include": "#output-form-label"
		}
	],
	"repository": {
		"title": {
			"match": "EthConverter:",
			"name": "markup.heading.1.html"
		},
		"number": {
			"match": "[0-9]+",
			"name": "constant.numeric"
		},
		"output-form-label": {
			"match": "(Usd|Ether|Gwei|Wei):",
			"name": "string.quoted"
		},
		"subheading": {
			"match": "(Ether|Gwei|Wei)[^:]",
			"name": "keyword.other"
		}
	}
}